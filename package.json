    "normalize.css": "^8.0.1",
    "pinia": "^2.0.30",
    "pinia-plugin-persist": "^1.0.0",
    "vant": "^4.0.10",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
